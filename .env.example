# Redis - Sistema de Cache
# Habilita/desabilita o cache Redis (true/false)
ENABLE_REDIS_CACHE=true

# URL de conexão do Redis (formato: redis://[:password@]host:port)
REDIS_URL=redis://:password@host:6379

# Senha do Redis (se necessário)
REDIS_PASSWORD=

# TTL padrão do cache em segundos (600 = 10 minutos)
REDIS_CACHE_TTL=600

# Memória máxima do Redis (ex: 256mb, 1gb)
REDIS_CACHE_MAX_MEMORY=256mb

# Habilita streaming de logs para Redis (opcional)
ENABLE_REDIS_LOG_STREAMING=false
