# Silenciar warnings de peer dependency do @excalidraw/excalidraw
# que requer React 17/18 mas usamos React 19
legacy-peer-deps=false
strict-peer-deps=false

# Silenciar warnings específicos de peer deps
# O @excalidraw/excalidraw@0.16.4 (via @platejs/excalidraw) não suporta React 19 ainda
loglevel=error

